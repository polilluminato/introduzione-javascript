<div class="container">
    <div class="row">
        <% data.forEach(function(singleCreditCard){ %>
            <div class="col-4">
                <div class="card" style="width: 18rem;">
                    <div class="card-body">
                      <h5 class="card-title"><%= singleCreditCard.number %></h5>
                    </div>
                  </div>
            </div>
        <% }); %>
    </div>
</div>